pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Hello this is building block'
            }
        }
    
        stage('prod'){
            steps {
                echo " this is production block"
            }
        }
    
       stage('test'){
        steps {
            echo "this is testing block"
        }
       }
    
    }
}